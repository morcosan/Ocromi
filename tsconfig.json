{
   "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "experimentalDecorators": true,
      "strict": true,
      "lib": [
         "es6",
         "dom",
         "es2017"
      ],
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
         "src/*": [
            "src/*"
         ]
      },
      "types": [
         "quasar",
         "jest"
      ],
   },
   "include": [
      "src/**/*"
   ],
   "exclude": [
      "docs",
      ".jest"
   ]
}
